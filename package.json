{
  "name": "alpha-generator",
  "version": "3.0.0",
  "description": "File Generator for Clean Code Projects",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run startnow",
    "build": "tsc -p .",
    "startnow": "set DEBUG=engine,socket* & node public/app.js",
    "dev": "nodemon public/app.js",
    "build_forever": "npm run build && forever start public/app.js",
    "build_dev": "npm run build && npm run dev",
    "build_x": "npm run build && 0x -o public/app.js",
    "build_pm2": "npm run build && pm2 start public/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@socket.io/redis-adapter": "^7.2.0",
    "@types/camelcase": "^5.2.0",
    "camelcase": "^7.0.0",
    "change-case": "^4.1.2",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "^16.0.1",
    "es6-template-string": "1.2.0",
    "express": "^4.18.1",
    "express-winston": "4.2.0",
    "helmet": "^5.1.0",
    "joi": "17.6.0",
    "morgan": "1.10.0",
    "morgan-body": "2.6.6",
    "multer": "^1.4.4-lts.1",
    "pm2": "5.2.0",
    "pug": "3.0.2",
    "spdy": "4.0.2",
    "tslib": "^2.4.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/apicache": "1.6.1",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-fileupload": "1.2.2",
    "@types/http-errors": "1.8.2",
    "@types/morgan": "1.9.3",
    "@types/multer": "1.4.7",
    "@types/node": "^17.0.36",
    "@types/spdy": "3.4.5",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "optionalDependencies": {
    "bufferutil": "4.0.5",
    "utf-8-validate": "5.0.5"
  }
}
